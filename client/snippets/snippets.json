{
    "begin": {
        "description": "BEGIN pattern",
        "prefix": "begin",
        "body": [
            "BEGIN {",
            "\t$0",
            "}"
        ]
    },
    "end": {
        "description": "END pattern",
        "prefix": "end",
        "body": [
            "END {",
            "\t$0",
            "}"
        ]
    },
    "beginfile": {
        "description": "BEGINFILE pattern",
        "prefix": "beginfile",
        "body": [
            "BEGINFILE {",
            "\t$0",
            "}"
        ]
    },
    "endfile": {
        "description": "ENDFILE pattern",
        "prefix": "endfile",
        "body": [
            "ENDFILE {",
            "\t$0",
            "}"
        ]
    },
    "not": {
        "description": "! operator",
        "prefix": "not",
        "body": [
            "!${1:expression}"
        ]
    },
    "compare to": {
        "description": "comparison operator",
        "prefix": "compare-to",
        "body": [
            "${1:expression} ${2|<,>,<=,>=,==,!=|} ${3:expression}"
        ]
    },
    "and": {
        "description": "&& operator",
        "prefix": "and",
        "body": [
            "${1:expression} && ${2:expression}"
        ]
    },
    "or": {
        "description": "|| operator",
        "prefix": "or",
        "body": [
            "${1:expression} || ${2:expression}"
        ]
    },
    "if": {
        "description": "if operator",
        "prefix": "if",
        "body": [
            "if (${1:condition})",
            "\t$0"
        ]
    },
    "if else": {
        "description": "if else operator",
        "prefix": "if-else",
        "body": [
            "if (${1:condition})",
            "\t${2:print}",
            "else",
            "\t$0",
            "fi"
        ]
    },
    "while": {
        "description": "while operator",
        "prefix": "while",
        "body": [
            "while (${1:condition})",
            "\t$0"
        ]
    },
    "do while": {
        "description": "do while operator",
        "prefix": "do-while",
        "body": [
            "do",
            "\t$0",
            "while (${1:condition})"
        ]
    },
    "for": {
        "description": "for operator",
        "prefix": "for",
        "body": [
            "for (${1:variable}=${2:value}; ${3:condition}; ${4:command})",
            "\t$0"
        ]
    }
}